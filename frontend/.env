REACT_APP_BACKEND_URL=http://localhost:8001

WDS_SOCKET_PORT=3000
REACT_APP_ENABLE_VISUAL_EDITS=false
ENABLE_HEALTH_CHECK=false

# Mock Mode Configuration
# Set to 'true' to use mock data without backend
# Set to 'false' to use real backend API (auto-fallback to mock if backend unavailable)
REACT_APP_USE_MOCK=true

# Google Analytics 4 Configuration
# Replace YOUR_GA4_MEASUREMENT_ID with your actual GA4 Measurement ID (G-XXXXXXXXXX)
# Get your Measurement ID from: https://analytics.google.com/
# Navigate to: Admin > Data Streams > Web > Measurement ID
REACT_APP_GA4_MEASUREMENT_ID=YOUR_GA4_MEASUREMENT_ID